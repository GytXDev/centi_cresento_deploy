{
    "root": true,
    "extends": "next",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "react/display-name": [
        "error",
        {
          "ignoreTranspilerName": false
        }
      ]
    }
  }
  